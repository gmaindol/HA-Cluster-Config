<high-availability>
<cluster>
        <enabled>yes</enabled>
        <cluster-members>
          <entry name="{{ serial_number }}">
            <ha4-ip-address>{{ ha4_ip_address }}</ha4-ip-address>
            <ha4-backup-ip-address>{{ ha4_backup_ip_address }}</ha4-backup-ip-address>
            <session-synchronization>enabled</session-synchronization>
          </entry>
        </cluster-members>
        <cluster-id>{{ cluster_id }}</cluster-id>
        <description>{{ ha_cluster_description }}</description>
      </cluster>
      <interface>
        <ha4>
          <port>{{ ha4_ethernet_interface }}</port>
          <ip-address>{{ ha4_ip_address }}</ip-address>
          <netmask>{{ ha4_netmask }}</netmask>
        </ha4>
        <ha4-backup>
          <port>{{ ha4_backup_ethernet_interface }}</port>
          <ip-address>{{ ha4_backup_ip_address }}</ip-address>
          <netmask>{{ ha4_backup_netmask }}</netmask>
        </ha4-backup>
      </interface>
<high-availability>